#!/usr/bin/gnuplot

reset

if (!exists("filename")) {
    exit
}

if (exists("out")) {
    set terminal png size 1024,848
    set output out
}

set title "NFA state labeling"
set xlabel "state index"
set ylabel "log(packets)/depth"
set grid
set style data lines

plot filename using 1:2 title "log packets", "" using 1:3 title "depth"

if (!exists("out")) {
    pause -1
}
